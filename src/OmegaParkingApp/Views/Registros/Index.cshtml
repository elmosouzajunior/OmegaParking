@model IEnumerable<OmegaParkingApp.ViewModels.RegistroViewModel>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.RegistroEntrada)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.RegistroSaida)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ValorCobrado)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.RegistroEntrada)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.RegistroSaida)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ValorCobrado)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
